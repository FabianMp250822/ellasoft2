rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow public read on logos and admin photos
    match /logos/{allPaths=**} {
      allow read;
      allow write: if request.auth != null;
    }
    match /admin_photos/{allPaths=**} {
      allow read;
      allow write: if request.auth != null;
    }
  }
}
